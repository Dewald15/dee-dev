{
    "name": "dee-dev",
    "version": "0.57.0",
    "repository": {
        "type": "git",
        "url": "git+ssh://gitlab.com/dee-dev/nx.git"
    },
    "license": "MIT",
    "scripts": {
        "// serve processes": "",
        "serve": "ng serve",
        "serve:dee-dev": "ng serve dee-dev",
        "// build processes": "",
        "build": "yarn build:papill-io:www",
        "build:dee-dev": "ng build dee-dev",
        "build:dee-dev:next": "ng build dee-dev --configuration=next",
        "build:dee-dev:prod": "ng build dee-dev --configuration=prod",
        "// testing": "",
        "test": "ng test",
        "test:all": "ng run cxfi:test-all",
        "test:all:affected": "yarn affected:test --files=package.json --parallel",
        "lint": "./node_modules/.bin/nx lint && ng lint",
        "e2e": "ng e2e",
        "affected:apps": "./node_modules/.bin/nx affected:apps",
        "affected:build": "./node_modules/.bin/nx affected:build",
        "affected:e2e": "./node_modules/.bin/nx affected:e2e",
        "test:ci": "echo 'skipping yarn test:all'",
        "affected:test": "./node_modules/.bin/nx affected:test",
        "affected:lint": "./node_modules/.bin/nx affected:lint",
        "// format": "",
        "format": "./node_modules/.bin/nx format:write --uncommitted",
        "format:write": "./node_modules/.bin/nx format:write",
        "format:check": "./node_modules/.bin/nx format:check",
        "// git processes": "",
        "git": "git add . && git commit -m \" dee-dev - commit\" && git push --set-upstream origin master",
        "// deployProcesses": "",
        "deploy": "yarn build:dee-dev && echo \"build done\" && firebase deploy && echo \"firebase deploy done\" && yarn git && echo \"commit done\" ",
        "// unused from spwn": "",
        "help": "./node_modules/.bin/nx help",
        "release": "./node_modules/.bin/semantic-release-gitlab",
        "update": "ng update @nrwl/schematics",
        "update:check": "ng update"
    },
    "private": true,
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "browser": {
        "zlib": false
    },
